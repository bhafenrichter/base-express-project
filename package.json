{
    "name": "kindness-project",
    "version": "0.0.1",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "nodemon",
        "watch": "concurrently \"webpack-dev-server --mode development --config webpack.dev.js\" \"nodemon\" -n browser,server -c \"bgBlue.bold,bgMagenta.bold\"",
        "build": "webpack --config webpack.dev.js",
        "deploy": "eslint src && webpack --config webpack.prd.js",
        "lint": "eslint src"
    },
    "author": "bhafenrichter",
    "license": "ISC",
    "eslintConfig": {
        "extends": "airbnb-base",
        "env": {
            "es6": true,
            "browser": true
        },
        "rules": {
            "brace-style": [
                "error",
                "stroustrup"
            ],
            "comma-dangle": [
                "error",
                "never"
            ],
            "no-unused-vars": [
                "warn"
            ],
            "no-var": [
                "off"
            ],
            "one-var": [
                "off"
            ]
        }
    },
    "dependencies": {
        "@tsconfig/node12": "^1.0.7",
        "@types/express": "^4.17.6",
        "@types/memory-cache": "^0.2.1",
        "@types/node-schedule": "^1.3.1",
        "concurrently": "^6.0.0",
        "express": "^4.17.1",
        "jquery": "^3.5.1",
        "memory-cache": "^0.2.0",
        "mustache-express": "^1.3.0",
        "node-schedule": "^2.0.0",
        "popper.js": "^1.16.1",
        "ts-node": "^9.1.1",
        "typescript": "^3.9.5"
    },
    "devDependencies": {
        "@analytics/google-analytics": "^0.3.1",
        "analytics": "^0.3.5",
        "bootstrap": "^4.5.0",
        "compression-webpack-plugin": "6.1.1",
        "copy-webpack-plugin": "^5.1.1",
        "css-loader": "^3.6.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.2.0",
        "eslint-plugin-import": "^2.21.2",
        "html-webpack-plugin": "^4.3.0",
        "image-webpack-loader": "^6.0.0",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "^4.14.1",
        "nodemon": "^2.0.4",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.2.1",
        "vanilla-lazyload": "^15.2.0",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0",
        "webpack-merge": "^4.2.2",
        "webpack-node-externals": "^1.7.2"
    }
}
