{
    "name": "kindness-project",
    "version": "0.0.1",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "nodemon app.js",
        "watch": "webpack-dev-server --config webpack.dev.js",
        "build": "webpack --config webpack.dev.js",
        "deploy": "eslint src && webpack --config webpack.prd.js",
        "lint": "eslint src"
    },
    "author": "bhafenrichter",
    "license": "ISC",
    "eslintConfig": {
        "extends": "airbnb-base",
        "env": {
            "es6": true,
            "browser": true
        },
        "rules": {
            "brace-style": [
                "error",
                "stroustrup"
            ],
            "comma-dangle": [
                "error",
                "never"
            ],
            "no-unused-vars": [
                "warn"
            ],
            "no-var": [
                "off"
            ],
            "one-var": [
                "off"
            ]
        }
    },
    "dependencies": {
        "express": "^4.17.1",
        "jquery": "^3.5.0",
        "mustache-express": "^1.3.0",
        "popper.js": "^1.16.1"
    },
    "devDependencies": {
        "@analytics/google-analytics": "^0.3.1",
        "analytics": "^0.3.5",
        "bootstrap": "^4.4.1",
        "compression-webpack-plugin": "^3.1.0",
        "copy-webpack-plugin": "^5.1.1",
        "css-loader": "^3.5.3",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.1.0",
        "eslint-plugin-import": "^2.20.2",
        "html-webpack-plugin": "^4.2.0",
        "image-webpack-loader": "^6.0.0",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "^4.14.0",
        "nodemon": "^2.0.3",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.2.0",
        "vanilla-lazyload": "^15.1.1",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3",
        "webpack-merge": "^4.2.2",
        "webpack-node-externals": "^1.7.2"
    }
}
